    <hint value="[Family name]"/>
  </variable>
  <variable>
    <name value="PatientSearchGivenName"/>
    <description value="Enter patient search criteria for a known given name on the target system"/>
    <hint value="[Given name]"/>
  </variable>
  <variable>
