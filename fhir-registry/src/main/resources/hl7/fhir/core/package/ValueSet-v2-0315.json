    "text": {
        "status": "additional",
        "div": "<div xmlns=\"http://www.w3.org/1999/xhtml\"><p>Living Will Code</p>\r\n<table class=\"grid\"><tr><td><b>Code</b></td><td><b>Description</b></td><td><b>Comment</b></td><td><b>Version</b></td></tr><tr><td>F<a name=\"F\"> </a></td><td>Yes, patient has a living will but it is not on file</td><td/><td>added v2.3</td></tr><tr><td>I<a name=\"I\"> </a></td><td>No, patient does not have a living will but information was provided</td><td/><td>added v2.3</td></tr><tr><td>N<a name=\"N\"> </a></td><td>No, patient does not have a living will and no information was provided</td><td/><td>added v2.3</td></tr><tr><td>U<a name=\"U\"> </a></td><td>Unknown</td><td/><td>added v2.3</td></tr><tr><td>Y<a name=\"Y\"> </a></td><td>Yes, patient has a living will</td><td/><td>added v2.3</td></tr></table>\r\n</div>"
